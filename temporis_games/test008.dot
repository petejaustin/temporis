// Benchmark game 8 - 10 vertices
// time_bound: 10
// targets: v7
digraph G {
    v0 [name="v0", player=0];
    v1 [name="v1", player=0];
    v2 [name="v2", player=1];
    v3 [name="v3", player=1];
    v4 [name="v4", player=1];
    v5 [name="v5", player=1];
    v6 [name="v6", player=1];
    v7 [name="v7", player=0, target=1];
    v8 [name="v8", player=0];
    v9 [name="v9", player=1];

    v0 -> v7 [constraint="t >= 4"];
    v1 -> v7 [constraint="t mod 2 == 0"];
    v2 -> v2;
    v3 -> v4 [constraint="t >= 1"];
    v4 -> v8;
    v5 -> v9;
    v6 -> v9 [constraint="t mod 5 == 3"];
    v7 -> v7 [constraint="t mod 5 == 2"];
    v8 -> v9;
    v9 -> v9;
    v3 -> v3;
    v4 -> v1;
    v2 -> v3;
    v2 -> v8;
    v1 -> v2;
}
