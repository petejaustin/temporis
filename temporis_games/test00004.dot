// Benchmark game 4 - 10 vertices
// time_bound: 10
// targets: v6
digraph G {
    v0 [name="v0", player=1];
    v1 [name="v1", player=0];
    v2 [name="v2", player=0];
    v3 [name="v3", player=1];
    v4 [name="v4", player=0];
    v5 [name="v5", player=1];
    v6 [name="v6", player=0, target=1];
    v7 [name="v7", player=1];
    v8 [name="v8", player=1];
    v9 [name="v9", player=0];

    v0 -> v1 [constraint="t mod 3 == 2"];
    v1 -> v0;
    v2 -> v9;
    v3 -> v6 [constraint="t < 10"];
    v4 -> v0;
    v5 -> v3;
    v6 -> v1;
    v7 -> v8;
    v8 -> v6;
    v9 -> v2;
    v2 -> v2;
    v6 -> v0;
    v2 -> v5;
    v6 -> v3;
    v4 -> v2;
    v1 -> v6;
}
