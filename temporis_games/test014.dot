// Benchmark game 14 - 10 vertices
// time_bound: 10
// targets: v5
digraph G {
    v0 [name="v0", player=0];
    v1 [name="v1", player=1];
    v2 [name="v2", player=0];
    v3 [name="v3", player=0];
    v4 [name="v4", player=0];
    v5 [name="v5", player=0, target=1];
    v6 [name="v6", player=1];
    v7 [name="v7", player=1];
    v8 [name="v8", player=0];
    v9 [name="v9", player=0];

    v0 -> v1 [constraint="t >= 2"];
    v1 -> v9 [constraint="t mod 2 == 0"];
    v2 -> v7;
    v3 -> v9;
    v4 -> v7;
    v5 -> v9;
    v6 -> v9;
    v7 -> v5 [constraint="t >= 4"];
    v8 -> v7;
    v9 -> v4;
    v8 -> v1;
    v7 -> v0;
    v0 -> v5;
    v4 -> v1;
    v9 -> v8;
}
