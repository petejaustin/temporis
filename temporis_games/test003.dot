// Benchmark game 3 - 10 vertices
// time_bound: 6
// targets: v6
digraph G {
    v0 [name="v0", player=0];
    v1 [name="v1", player=1];
    v2 [name="v2", player=1];
    v3 [name="v3", player=0];
    v4 [name="v4", player=0];
    v5 [name="v5", player=1];
    v6 [name="v6", player=0, target=1];
    v7 [name="v7", player=0];
    v8 [name="v8", player=0];
    v9 [name="v9", player=0];

    v0 -> v3 [constraint="t < 12"];
    v1 -> v3;
    v2 -> v0 [constraint="t >= 4"];
    v3 -> v4;
    v4 -> v7 [constraint="t mod 5 == 1"];
    v5 -> v3 [constraint="t >= 5"];
    v6 -> v8 [constraint="t < 5"];
    v7 -> v0;
    v8 -> v8;
    v9 -> v8 [constraint="t mod 2 == 0"];
    v9 -> v1;
    v3 -> v6;
    v1 -> v9;
    v3 -> v9;
    v9 -> v0;
}
