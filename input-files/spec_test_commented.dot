digraph spec_test_commented {
    /*
     * Comprehensive Specification Test
     * 
     * This file demonstrates all supported operations in the Presburger arithmetic
     * temporal reachability game system. Each edge showcases different operations
     * while maintaining proper existential quantification and temporal reasoning.
     */
    
    // Vertices
    v0 [name="v0", player=0, target=1];  // Target vertex
    v1 [name="v1", player=1];            // Player 1 choice point
    v2 [name="v2", player=0];            // Player 0 intermediate
    v3 [name="v3", player=1];            // Player 1 intermediate  
    v4 [name="v4", player=0];            // Player 0 intermediate
    v5 [name="v5", player=1];            // Player 1 intermediate
    v6 [name="v6", player=0];            // Player 0 intermediate
    v7 [name="v7", player=1];            // Player 1 intermediate
    v8 [name="v8", player=0];            // Player 0 intermediate
    v9 [name="v9", player=1];            // Player 1 return point
    
    // Edge 1: AND operation with >= and + operation
    v0 -> v1 [constraint="exists x y. (time >= 2 && x + y >= 10 && x >= 5)"];
    
    // Edge 2: OR operation with <= and modulus
    v1 -> v2 [constraint="exists x y. (time <= 15 && (x % 3 == 1 || y <= 7))"];
    
    // Edge 3: NOT operation with > and + operation
    v2 -> v3 [constraint="exists x y z. (time > 5 && !(x + y > z + 3))"];
    
    // Edge 4: Complex AND/OR with < and modulus
    v3 -> v4 [constraint="exists x y. (time < 20 && (x % 2 == 0 && y < 15) || time >= 10)"];
    
    // Edge 5: NOT with modulus and <=
    v4 -> v5 [constraint="exists x y. (time <= 25 && !(x % 4 == 2) && y + 3 <= 12)"];
    
    // Edge 6: >= with complex + operations and AND
    v5 -> v6 [constraint="exists x y z. (time >= 8 && x + 2*y + z >= 20 && x >= 0)"];
    
    // Edge 7: OR with > and NOT of modulus
    v6 -> v7 [constraint="exists x y. (time > 12 && (x > y || !(x % 5 == 0)))"];
    
    // Edge 8: Complex nested logic with all operations
    v7 -> v8 [constraint="exists x y z w. (time >= 15 && ((x + y > 10 && z % 3 == 2) || !(w <= 5)) && time < 30)"];
    
    // Edge 9: < with AND and + operation
    v8 -> v9 [constraint="exists x y. (time < 35 && x + y < 25 && x >= y)"];
    
    // Edge 10: Return edge with <= and OR
    v9 -> v0 [constraint="exists x. (time <= 40 && (x <= 0 || time >= 30))"];
}
