digraph SixVariableOptimal {
    // 6 variables - targeting ~1 minute completion time
    v0 [name="v0", player=0];
    v1 [name="v1", player=1];
    
    // 6 variables: time = a + b + c + d + e + f + 12
    v0 -> v1 [label="e0", constraint="exists a. exists b. exists c. exists d. exists e. exists f. time = a + b + c + d + e + f + 12"];
    
    // Simple return
    v1 -> v0 [label="e1", constraint="time >= 5"];
}
