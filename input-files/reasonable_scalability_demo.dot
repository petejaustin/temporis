digraph ReasonableScalabilityDemo {
    v0 [name="v0", player=0];
    v1 [name="v1", player=1];
    v2 [name="v2", player=0];
    
    // 3 variables: time = 2*a + 3*b + c + 1
    v0 -> v1 [label="e0", constraint="exists a. exists b. exists c. time = 2*a + 3*b + c + 1"];
    
    // 4 variables: time = w + x + y + z + 5
    v1 -> v2 [label="e1", constraint="exists w. exists x. exists y. exists z. time = w + x + y + z + 5"];
    
    // Return path
    v2 -> v0 [label="e2", constraint="time <= 8"];
}
