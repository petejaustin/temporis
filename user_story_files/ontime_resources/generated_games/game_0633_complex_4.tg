// Complex graph with 4 nodes

node s0: label["start"], owner[0]
node s1: label["n1"], owner[0]
node s2: label["n2"], owner[1]
node s3: label["target"], owner[0]

edge s0 -> s1: (not (= (mod t 3) 2))
edge s1 -> s2
edge s2 -> s3: (not (= (mod t 5) 2))
edge s3 -> s1: (or (= (mod t 3) 2) (= (mod t 2) 0))
