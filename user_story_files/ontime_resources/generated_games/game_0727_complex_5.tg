// Complex graph with 5 nodes

node s0: label["start"], owner[0]
node s1: label["n1"], owner[0]
node s2: label["c"], owner[1]
node s3: label["d"], owner[1]
node s4: label["target"], owner[0]

edge s0 -> s1
edge s1 -> s2: (not (= (mod t 5) 1))
edge s1 -> s3: (= (mod t 3) 2)
edge s3 -> s2: (and (= (mod t 4) 2) (= (mod t 3) 0))
