// Complex graph with 5 nodes

node s0: label["start"], owner[0]
node s1: label["b"], owner[0]
node s2: label["c"], owner[1]
node s3: label["n3"], owner[0]
node s4: label["target"], owner[0]

edge s0 -> s2: (not (= (mod t 3) 0))
edge s3 -> s1
edge s4 -> s0: (and (= (mod t 4) 3) (= (mod t 4) 1))
edge s4 -> s1
edge s4 -> s3: (and (= (mod t 2) 0) (= (mod t 3) 2))
