// Simple chain graph with 5 nodes

node s0: label["a"], owner[0]
node s1: label["b"], owner[1]
node s2: label["c"], owner[0]
node s3: label["d"], owner[1]
node s4: label["e"], owner[0]

edge s0 -> s1: (= (mod t 5) 1)
edge s1 -> s2: (= (mod t 3) 1)
edge s2 -> s3: (7,8,10,13,14)
edge s3 -> s4: (0,3,4,5,11)
edge s1 -> s1: (= (mod t 3) 0)
edge s3 -> s2: (not (= (mod t 3) 0))
