// Cycle graph with 5 nodes

node s0: label["c0"], owner[0]
node s1: label["c1"], owner[1]
node s2: label["c2"], owner[0]
node s3: label["c3"], owner[0]
node s4: label["c4"], owner[1]

edge s0 -> s1: (= (mod t 5) 4)
edge s1 -> s2: (= (mod t 2) 0)
edge s2 -> s3: (not (= (mod t 3) 0))
edge s3 -> s4: (= (mod t 4) 2)
edge s4 -> s0: (= (mod t 2) 1)
edge s2 -> s2: (= (mod t 4) 0)
