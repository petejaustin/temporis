// Complex graph with 4 nodes

node s0: label["s0"], owner[0]
node s1: label["s1"], owner[1]
node s2: label["s2"], owner[1]
node s3: label["s3"], owner[0]

edge s1 -> s2: (or (= (mod t 5) 0) (= (mod t 4) 1))
edge s1 -> s3: (= (mod t 5) 1)
edge s2 -> s3
edge s3 -> s2: (or (= (mod t 5) 3) (= (mod t 2) 1))
